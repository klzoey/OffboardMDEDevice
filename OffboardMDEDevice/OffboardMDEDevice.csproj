<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>MDEOffboard.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="exit_icon.png" />
    <None Remove="MDEOffboard.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="MDEOffboard.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Core" Version="1.41.0" />
    <PackageReference Include="Azure.Identity" Version="1.12.0" />
    <PackageReference Include="Azure.Security.KeyVault.Secrets" Version="4.6.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="exit_icon.png" />
    <Resource Include="MDEOffboard.png" />
  </ItemGroup>

</Project>
